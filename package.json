{
  "name": "@antongolub/lockfile",
  "version": "0.0.0-snapshot.4",
  "publishConfig": {
    "access": "public"
  },
  "description": "Read and write lockfiles with reasonable losses",
  "main": "./target/es6/index.js",
  "type": "module",
  "exports": "./target/es6/index.js",
  "dependencies": {
    "@semrel-extra/topo": "^1.13.1",
    "js-yaml": "^4.1.0",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.5.0",
    "@types/semver": "^7.5.0",
    "esbuild": "^0.19.2",
    "esbuild-node-externals": "^1.8.0",
    "tsm": "^2.3.0",
    "typescript": "^5.1.6",
    "uvu": "^0.5.6"
  },
  "scripts": {
    "build": "yarn build:es6 && yarn build:libdef",
    "build:es6": "node ./build.js",
    "build:libdef": "tsc --emitDeclarationOnly --outDir target/es6",
    "build:esmfix": "tsc-esm-fix --target=target/es6 --ext=.mjs",
    "test": "yarn test:unit",
    "test:unit": "uvu -r tsm -i helpers 'src/test/ts/'",
    "publish:snapshot": "npm publish --no-git-tag-version --tag snapshot"
  },
  "files": [
    "target",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antongolub/lockfile.git"
  },
  "keywords": [
    "lock",
    "lockfile",
    "yarn",
    "yarnlock",
    "yarn.lock",
    "package-lock",
    "npm"
  ],
  "author": "Anton Golub <antongolub@antongolub.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/antongolub/lockfile/issues"
  },
  "homepage": "https://github.com/antongolub/lockfile#readme",
  "packageManager": "yarn@3.1.1"
}
